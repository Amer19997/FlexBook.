                                    <div class="d-flex">
                                        <span>
                                            <img src="~/Cms_Lib/img/icons/bullet.svg"
                                             alt=""
                                             width="13px"
                                             height="13px" />
                                        </span>
                                        <p class="me-1">بيانات الحسابات البنكية للجهة</p>
                                    </div>
                                    <div class="col-12">
                                        <div class="row">
                                            <div class="col-10 m-auto">

                                                <!--strat table -->
                                                <div class="app-table table-responsive">
                                                    <table class="table table-bordered">
                                                        <thead>
                                                            <tr class="table-header">
                                                                <th scope="col">م</th>
                                                                <th scope="col">اسم البنك</th>
                                                                <th scope="col">رقم الحساب البنكي</th>
                                                                <th scope="col">
                                                                    رقم حساب الآيبان البنكي
                                                                </th>
                                                                <th scope="col">
